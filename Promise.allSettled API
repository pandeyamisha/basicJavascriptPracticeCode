const p1= new Promise(function(resolve,reject){
  setTimeout(function(){
  resolve("P1 Success")
  },3000)
})
const p2= new Promise(function(resolve,reject){
  setTimeout(function(){
  resolve("P2 Success")
  },5000)
})
const p3= new Promise(function(resolve,reject){
  setTimeout(function(){
  resolve("P3 Success")
  },1000)
})
Promise.allSettled([p1,p2,p3])
.then((res) =>{
  console.log(res)
})
.catch((err)=>{
  console.error(err);
})


FAILURE CASE

const p4= new Promise(function(resolve,reject){
  setTimeout(function(){
  resolve("P4 Failed")
  },3000)
})
const p5= new Promise(function(resolve,reject){
  setTimeout(function(){
  resolve("P5 Failed")
  },9000)
})
const p6= new Promise(function(resolve,reject){
  setTimeout(function(){
  reject("P6 Failed")
  },1000)
})
Promise.allSettled([p4,p5,p6])
.then((res) =>{
  console.log(res)
})
.catch((err)=>{
  console.error(err);
})
